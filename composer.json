{
  "name": "jorisros/data-compare",
  "description": "Component that is able to compare big data sets",
  "minimum-stability": "stable",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Joris Ros",
      "email": "joris.ros@gmail.com"
    }
  ],
  "require": {
    "phpunit/phpunit": "^7.5.14 || ^8.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "DataCompare\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "cs": "vendor/bin/phpcs --standard=PSR12 src",
    "test": "vendor/bin/phpunit"
  }
}
